    "n = int(input())\n",
    "a = [int(x) for x in input().split()]\n",
    "largest = secondlargest = -100\n",
    "for x in a:\n",
    "    if x > largest:\n",
    "        tmp = largest\n",
    "        largest = x\n",
    "        secondlargest = tmp\n",
    "    elif x > secondlargest and x != largest:\n",
    "        secondlargest = x\n",
    "print(secondlargest)"
   ]
