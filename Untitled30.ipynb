    "def speak(str):\n",
    "    from win32com.client import Dispatch\n",
    "    speak = Dispatch(\"SAPI.spVoice\")\n",
    "    speak.Speak(str)\n",
    "    \n",
    "    if __init__ == '__main__':\n",
    "        speak(\"papa kaaak kakaka\")\n"
   ]
